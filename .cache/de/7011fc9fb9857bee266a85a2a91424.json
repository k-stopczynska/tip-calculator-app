n  justify-content: end;\r\n  align-items: center;\r\n  min-height: 100vh;\r\n  width: 100vw;\r\n  background-color: $light-grayish-cyan;\r\n  @include md {\r\n    justify-content: center;\r\n  }\r\n}\r\n\r\nheader {\r\n  margin: 3rem auto;\r\n  position: fixed;\r\n  top: 0;\r\n  opacity: 0.7;\r\n  @include lg {\r\n    margin: 5em auto;\r\n  }\r\n}\r\n\r\nfooter {\r\n  position: fixed;\r\n  bottom: 0;\r\n  width: 100%;\r\n  height: 15px;\r\n}\r\n\r\n.container {\r\n  background-color: $white;\r\n  width: 100%;\r\n  border-radius: $wrapper-border-radius;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: space-between;\r\n  justify-content: center;\r\n  padding: 1.5em;\r\n\r\n  @include md {\r\n    flex-direction: row;\r\n    gap: 1em;\r\n    min-height: 30vh;\r\n    max-width: 90vw;\r\n  }\r\n\r\n  @include lg {\r\n    max-width: 80vw;\r\n    max-height: 55vh;\r\n    gap: 2em;\r\n  }\r\n  @include xl {\r\n    max-width: 60vw;\r\n  }\r\n}\r\n\r\n.user-input-container {\r\n  @include md {\r\n    width: 50%;\r\n    padding: 1em;\r\n  }\r\n}\r\n\r\n.bill-input-wrapper,\r\n.tip-input-wrapper,\r\n.people-input-wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n  padding-bottom: 1.5em;\r\n}\r\n\r\n.people-input-wrapper {\r\n  @include md {\r\n    padding-bottom: 0.5em;\r\n  }\r\n}\r\n\r\n.buttons-wrapper {\r\n  display: flex;\r\n  gap: 0.5em;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.output-container {\r\n  background-color: $very-dark-cyan;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1.5em;\r\n  padding: 1.5em;\r\n  border-radius: $wrapper-border-radius;\r\n  @include md {\r\n    width: 50%;\r\n    padding: 2.5em;\r\n    justify-content: space-between;\r\n  }\r\n}\r\n\r\n.tip-output-wrapper,\r\n.total-output-wrapper {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.tip-output-wrapper {\r\n  @include lg {\r\n    margin: 2em auto;\r\n  }\r\n}\r\n\r\n.total-output,\r\n.tip-output {\r\n  displa